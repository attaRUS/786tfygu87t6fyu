run_log "Test common Lua script"
responds_with \
    "Foo: bar" \
    "http://localhost:${PORT_PROXY}"

run_log "Test route-specific Lua script"
responds_with_header \
    "header_key_1: header_value_1" \
    "http://localhost:${PORT_PROXY}/multiple/lua/scripts"

